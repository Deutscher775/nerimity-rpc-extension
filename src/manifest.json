{
    "manifest_version": 3,
   
    "name": "Nerimity RPC",
    "version": "0.1.1",
    "description": "Get rich presence from Nerimity. Show what you're doing on Nerimity!",


    "web_accessible_resources": [{
      "matches": [
        "https://open.spotify.com/*"
      ],
      "resources": ["WebSocketRPC.js"]
    }],

    
    "content_scripts": [{
      "js": ["content.js"],
      "run_at": "document_start",
      "matches": [
        "https://open.spotify.com/*"
      ]
    }]
   
  }